[Unit]
Description=gunicorn_default daemon
Requires=gunicorn_default.socket
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/var/www/django_apps/default/default
ExecStart=/var/www/django_apps/default/env/bin/gunicorn \
          --access-logfile - \
          --workers 5 \
          --bind unix:/run/gunicorn_default.sock \
          default.wsgi:application

[Install]
WantedBy=multi-user.target
