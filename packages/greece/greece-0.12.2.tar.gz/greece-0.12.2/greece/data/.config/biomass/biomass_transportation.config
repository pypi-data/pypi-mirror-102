# Default biomass transportation config file for GREECE model

# This file implements parameters for biomass.py script
# Biomass transportation model
#
# All text after a hash (#) is regarded as a comment and
# will be ignored

#------------------------------------------
#                 Model options
#------------------------------------------

# When projecting polygons (resource and generation) onto network,
# should we project polygon's centroid ? {true, {false}}

USE_CENTROID_PROJECTION =


#------------------------------------------
#           Vehicle characteristics
#------------------------------------------

# Time format (travel time output in seconds, minutes or hours) {'s', 'm', 'h'}

TIME_FORMAT =

# Tare weight (unloaded; common values: 13600-15400 kg)

TARE_WEIGHT =

# Gross weight (loaded; common values: 30000-40000 kg)

GROSS_WEIGHT =

# Gross horse power (common value: 500)

GROSS_HORSE_POWER =

# Frontal area (common values: 2.52m² for cars, 5.13m² for vans, 8.67m² for tourist coaches and 7.92m² for trucks)

FRONTAL_AREA =

# Engine efficiency (0.27: gasoline, 0.40: diesel)

ENGINE_EFFICIENCY =

# Fuel volumetric energy density (gasoline: 32.18 MJ/L, diesel: 35.86 MJ/L)

FUEL_VOLUMETRIC_ENERGY_DENSITY =

# Uphill horse power rate {0.8}

UPHILL_HORSE_POWER_RATE =

# Downhill horse power rate {0.6}

DOWNHILL_HORSE_POWER_RATE =

# Drag resistance {0.35}

DRAG_RESISTANCE =

# Mass correction factor {1.05}

MASS_CORRECTION_FACTOR =

# Acceleration rate {0.5}

ACCELERATION_RATE =

# Deceleration rate {3}

DECELERATION_RATE =
